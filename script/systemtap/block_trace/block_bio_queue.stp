probe begin {
    printf("probe trace block_bio_queue\n")
}

probe kernel.trace("block_bio_queue") {
    dev_t = $bio->bi_bdev->bd_dev
    major = MAJOR(dev_t)
    minor = MINOR(dev_t)
    if (major == 8 && minor == 16) {
        printf("(%u, %u) sector:%d opf:0x%x comm:%s\n",
               major, minor, $bio->bi_iter->bi_sector, $bio->bi_opf, execname())
    }
}
